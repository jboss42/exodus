    HRESULT _stdcall showToast([in] BSTR Message, [in] long wndHandle, [in] long imageIndex );
    /**
        \brief Changes the presence of the session JID.

        This method changes the presence of this session JID.

        \param Show The new show value describing the availability (show, away, dnd, xa).
        \param Status The new status value (natural language description of the show state).
        \param Priority The new priority (-128 to +127).
        \return \c HRESULT either \c S_OK or an error value.
    **/
    [
    id(0x00000033)
    ]
    HRESULT _stdcall setPresence([in] BSTR Show, [in] BSTR Status, [in] long Priority );
    /**
        \brief Returns the Roster interface.

        This method returns the roster interface.

        \param Value The IExodusRoster interface.
        \return \c HRESULT either \c S_OK or an error value.
    **/
    [
    propget,
    id(0x00000036)
    ]
    HRESULT _stdcall Roster([out, retval] IExodusRoster ** Value /*Warning: unable to validate structure name: */ );
    /**
        \brief Returns the presence database interface.

        This method returns the presence database interface.

        \param Value The IExodusPPDB interface.
        \return \c HRESULT either \c S_OK or an error value.
    **/
    [
    propget,
    id(0x00000037)
    ]
    HRESULT _stdcall PPDB([out, retval] IExodusPPDB ** Value /*Warning: unable to validate structure name: */ );
